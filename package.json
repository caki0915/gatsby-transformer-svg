{
  "name": "gatsby-transformer-svg",
  "version": "1.0.0",
  "description": "Gatsby transformer plugin for SVG files",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.6",
    "mini-svg-data-uri": "^1.1.3",
    "svgo": "^1.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "babel-preset-gatsby-package": "^0.2.15",
    "cross-env": "^5.2.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "babel-eslint": "^8.0.1",
    "eslint-plugin-flowtype": "^2.39.1",
    "prettier-eslint": "^8.2.0",
    "prettier-eslint-cli": "^4.4.0"
  },
  "peerDependencies": {
    "gatsby": "^2.0.15"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caki0915/gatsby-transformer-svg"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "svg"
  ],
  "files": ["gatsby-node.js"],
  "author": "Carl-Johan Kihl",
  "license": "ISC"
}
